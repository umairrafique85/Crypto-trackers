   "source": [
    "library(httr)\n",
    "library(jsonlite)\n",
    "\n",
    "url_coinmarcap <- \"https://api.coinmarketcap.com\"\n",
    "path_ticker <- \"v1/ticker/\"\n",
    "\n",
    "raw.data <- GET(url = url_coinmarcap, path = path_ticker)\n",
    "raw.content <- rawToChar(raw.data$content)\n",
    "\n",
    "neat_content <- fromJSON(raw.content)\n",
    "\n",
    "head(neat_content)\n"
   ]
